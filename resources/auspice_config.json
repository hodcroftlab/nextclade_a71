{
  "title": "Enterovirus A71 - Nextclade dataset tree",
  "build_url": "https://github.com/hodcroftlab/nextclade_a71",
  "maintainers": [
    {
      "name": "Nadia Neuner-Jehle",
      "url": "https://www.swisstph.ch/en/staff/profile/people/nadia-neuner-jehle"
    },
    {
      "name": "Emma Hodcroft",
      "url": "http://emmahodcroft.com/"
    }
  ],
  "extensions": {
    "nextclade": {
      "ref_nodes": {
        "default": "__root__",
        "builtins": {
          "__root__": {
            "displayName": "Reference U22521.1",
            "description": "BrCr strain, complete cds"
          }
        },
        "search": [
          {
            "name": "InferredAncestor",
            "displayName": "Static Inferred Ancestor",
            "description": "Precomputed ancestral sequence (static)",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["ancestral_sequence"]
                  }
                ]
              }
            ]
          },
          {
            "name": "Inferred root",
            "displayName": "Tree root",
            "description": "Inferred ancestral root (regenerated on each run)",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["NODE_0000000"]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },
  "colorings": [
    {
      "title": "Clade",
      "type": "categorical",
      "key": "clade_membership"
    },
    {
      "title": "Collection Date",
      "type": "categorical",
      "key": "date"
    },
    {
      "title": "Country",
      "type": "categorical",
      "key": "country"
    },
    {
      "title": "Global Region",
      "type": "categorical",
      "key": "region"
    }
    ],
  "geo_resolutions": [
    "country", "region"
  ],
  "filters": [
    "country",
    "region",
    "clade_membership"
  ],
  "display_defaults": {
    "color_by": "clade_membership",
    "geo_resolution": "country",
    "tip_label": "strain",
    "branch_label": "clade",
    "map_triplicate": true
  },
  "metadata_columns": [
    "strain"
  ],
  "panels": [
    "tree"
  ]
}
