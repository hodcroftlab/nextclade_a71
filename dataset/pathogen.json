{
  "schemaVersion": "3.0.0",
  "files": {
    "reference": "reference.fasta",
    "pathogenJson": "pathogen.json",
    "genomeAnnotation": "genome_annotation.gff3",
    "treeJson": "tree.json",
    "examples": "sequences.fasta",
    "readme": "README.md",
    "changelog": "CHANGELOG.md"
  },
  "attributes": {
    "name": "Enterovirus A71",
    "reference name": "Human enterovirus 71 polyprotein gene, complete cds.",
    "reference accession": "U22521.1"
  },
  "alignmentParams": {
      "penaltyGapExtend": 1,
      "penaltyGapOpen": 6,
      "penaltyGapOpenInFrame": 12,
      "penaltyGapOpenOutOfFrame": 18,
      "kmerLength": 8,
      "kmerDistance": 30,
      "minMatchLength": 40,
      "allowedMismatches": 8,
      "minLength": 100,
      "gapAlignmentSide": "left",
      "minSeedCover": 0.3
  },
  "defaultCds": "VP1",
  "compatibility": {
    "cli": "3.0.0",
    "web": "3.0.0"
  },
  "qc": {
    "frameShifts": {
      "enabled": true,
      "scoreWeight": 100
    },
    "missingData": {
      "enabled": true,
      "missingDataThreshold": 1000,
      "scoreBias": 100
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 10
    },
    "privateMutations": {
      "cutoff": 120,
      "enabled": true,
      "typical": 40,
      "weightLabeledSubstitutions": 1.5,
      "weightReversionSubstitutions": 2,
      "weightUnlabeledSubstitutions": 1
    },
    "divergence": {
      "enabled": true,
      "maxDivergence": 0.15,
      "scoreWeight": 50
    },
    "stopCodons": {
      "enabled": true,
      "scoreWeight": 60
    },
    "recombinants": {
      "enabled": true,
      "mutationsThreshold": 140,
      "scoreWeight": 50
    }
  },
  "cdsOrderPreference":["VP4", "VP2", "VP3", "VP1", "2A", "2B", "2C", "3A", "3B", "3C", "3D"]

}